    },
    {
      "name": "list",
      "path": "/api/v2/webdeployments/deployments",
      "description": "Get deployments"
    },
    {
      "name": "create",
      "path": "/api/v2/webdeployments/deployments",
      "description": "Create a deployment"
    }
