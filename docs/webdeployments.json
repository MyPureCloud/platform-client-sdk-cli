    {
      "name": "get",
      "path": "/api/v2/webdeployments/deployments/{deploymentId}",
      "description": "Get a deployment"
    },
    {
      "name": "update",
      "path": "/api/v2/webdeployments/deployments/{deploymentId}",
      "description": "Update a deployment"
    },
    {
      "name": "delete",
      "path": "/api/v2/webdeployments/deployments/{deploymentId}",
      "description": "Delete a deployment"
    },
    {
      "name": "get",
      "path": "/api/v2/webdeployments/deployments/{deploymentId}/configurations",
      "description": "Get active configuration for a given deployment"
    },
    {
      "name": "delete",
      "path": "/api/v2/webdeployments/token/revoke",
