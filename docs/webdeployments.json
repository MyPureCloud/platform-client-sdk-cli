    {
      "name": "get",
      "path": "/api/v2/webdeployments/configurations/{configurationId}/versions/draft",
      "description": "Get the configuration draft"
    },
    {
      "name": "update",
      "path": "/api/v2/webdeployments/configurations/{configurationId}/versions/draft",
      "description": "Update the configuration draft"
    },
    {
      "name": "create",
      "path": "/api/v2/webdeployments/configurations/{configurationId}/versions/draft/publish",
      "description": "Publish the configuration draft and create a new version"
    },
    {
