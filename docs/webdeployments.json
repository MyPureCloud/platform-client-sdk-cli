      "description": "Retrieves a cobrowse session"
    },
    {
      "name": "delete",
      "path": "/api/v2/webdeployments/deployments/{deploymentId}/cobrowse/{sessionId}",
      "description": "Deletes a cobrowse session"
    },
    {
      "name": "create",
      "path": "/api/v2/webdeployments/token/refresh",
      "description": "Refresh a JWT."
    },
    {
      "name": "create",
      "path": "/api/v2/webdeployments/token/oauthcodegrantjwtexchange",
      "description": "Exchange an oAuth code (obtained using the Authorization Code Flow or Implicit flow) for a JWT that can be used by webdeployments."
    },
    {
      "name": "delete",
      "path": "/api/v2/webdeployments/configurations/{configurationId}",
      "description": "Delete all versions of a configuration"
    },
    {
      "name": "list",
      "path": "/api/v2/webdeployments/configurations",
      "description": "View configuration drafts"
